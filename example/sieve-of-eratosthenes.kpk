(ns sieve-of-eratosthenes
  (require lists)
  (use (io :only (format)))
  (use (erlang :only (:'/='/2 rem/2 :'+'/2))))

(defn- sieve ([] primes)
  (lists.reverse primes))

(defn- sieve ([h & t] primes)
  (sieve (lists.filter (fn [x]
                         (:'/=' (rem x h) 0))
                       t)
         [h & primes]))

(defn sieve (v)
  (sieve (lists.seq 2 v) []))

(defn main ()
  (let [v 1000]
    (format 'run sieve(~B) return: ~p~n' [v (sieve v)])))
